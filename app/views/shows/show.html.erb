<h1><%= @show.name %></h1>
<h3>Venue: <%= @show.venue %></h3>
<h3>Client: <%= @show.client %></h3>
<h3>Start: <%= @show.start %></h3>
<h3>End: <%= @show.end %></h3>
<%= link_to 'Edit', edit_show_path(@show) %>
<%= link_to 'Delete', show_path(@show), method: "delete", data: { confirm: "Are you sure you want to delete #{@show.name}?" }%>

