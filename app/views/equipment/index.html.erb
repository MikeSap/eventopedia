<table class="table table-striped table-bordered table-sm">
    <thead class="thead-dark">
        <tr>
            <th>Equipment Name</th>
            <th>Quantity Owned</th>
            <th>Category</th>
            <th>SubCategory</th>
        </tr>
    </thead>
    <tbody>
<% @equipment.each do |e|%>
    <tr>
    <td><%= link_to e.name, equipment_path(e) %></td>
    <td> <%= e.quantity %> </td>
    <td> <%= e.category %> </td>
    <td> <%= e.sub_category %> </td>
    </tr>
<% end %>
</tbody>
</table>
<%= link_to 'Add Equipment', new_equipment_path %>